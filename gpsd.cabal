name:                gpsd
version:             1.0.0
synopsis:            A Haskell gateway to GPSD
-- description:         
homepage:            https://github.com/CodeBlock/gpsd-haskell
license:             BSD3
license-file:        LICENSE
author:              Ricky Elrod
maintainer:          ricky@elrod.me
-- copyright:           
category:            Network
build-type:          Simple
-- extra-source-files:  
cabal-version:       >=1.10

library
  exposed-modules:     Main
  -- other-modules:       
