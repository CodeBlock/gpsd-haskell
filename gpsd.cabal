  build-depends:       base >=4.6 && <4.7, io-streams >= 1.1 && < 1.2, network >= 2.4 && < 2.5, aeson >= 0.7 && < 0.8, bytestring >= 0.10 && < 0.11, containers >= 0.5 && < 1, lens >= 4 && < 5
  hs-source-dirs:      src
  default-language:    Haskell2010
